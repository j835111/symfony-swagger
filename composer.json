{
    "name": "j835111/symfony-swagger-bundle",
    "description": "Symfony Bundle for Swagger/OpenAPI documentation integration",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "swagger", "openapi", "api", "documentation"],
    "authors": [
        {
            "name": "j835111"
        }
    ],
    "homepage": "https://github.com/j835111/symfony-swagger-bundle",
    "support": {
        "issues": "https://github.com/j835111/symfony-swagger-bundle/issues"
    },
    "require": {
        "php": ">=8.1",
        "psr/log": "^3.0",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/config": "^6.0|^7.0",
        "symfony/routing": "^6.0|^7.0",
        "symfony/cache-contracts": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.10"
    },
    "scripts": {
        "analyze": [
            "phpstan analyse",
            "php-cs-fixer fix --dry-run --diff"
        ]
    },
    "autoload": {
        "psr-4": {
            "SymfonySwagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonySwagger\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
