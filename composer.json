{
    "name": "your-vendor/symfony-swagger-bundle",
    "description": "Symfony Bundle for Swagger/OpenAPI documentation integration",
    "type": "symfony-bundle",
    "license": "MIT",
    "keywords": ["symfony", "swagger", "openapi", "api", "documentation"],
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.0|^7.0",
        "symfony/http-kernel": "^6.0|^7.0",
        "symfony/dependency-injection": "^6.0|^7.0",
        "symfony/config": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^6.0|^7.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "SymfonySwagger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SymfonySwagger\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
